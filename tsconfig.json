{
  "compilerOptions": {
    "baseUrl": "./src",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "paths": {
      "@views/*": [".views/*"],
      "@pages/*": [".pages/*"],
      "@components/*": [".components/*"],
      "@context/*": [".context/*"],
      "@store/*": [".store/*"],
      "@utils/*": [".utils/*"],
      "@assets/*": [".assets/*"],
      "@data/*": [".data/*"]
    }
  },
  "include": ["src"]
}
